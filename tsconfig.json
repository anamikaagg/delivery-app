{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "/*"
      ],
      "@api/*": [
        "api/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@modals/*": [
        "modals/*"
      ],
      "@redux/*": [
        "redux/*"
      ],
      "@screens/*": [
        "screens/*"
      ],
      "@env": [
        ".env"
      ]
    },
    "noImplicitAny": true,
    "typeRoots": [
      "src/types/env.d.ts"
    ],
    "esModuleInterop": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts"
  ]
}